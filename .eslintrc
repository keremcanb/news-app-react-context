{
  "extends": ["wesbos"],
  "rules": {
    "react/no-danger": 0,
    "react-hooks/exhaustive-deps": 0,
    "camelcase": 0,
    "react/prop-types": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}
