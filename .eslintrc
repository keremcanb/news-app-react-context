{
  "extends": ["wesbos"],
  "rules": {
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/display-name": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "react/no-danger": 0,
    "arrow-body-style": 0,
    "no-case-declarations": 0,
    "react/button-has-type": 0,
    "react/prop-types": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}
